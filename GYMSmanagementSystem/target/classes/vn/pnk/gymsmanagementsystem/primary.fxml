      <Label text="Primary View" />
      <Button fx:id="primaryButton" onAction="#switchToSecondary" style="-fx-background-color: #6DB5CA;" text="Switch to Secondary View" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
